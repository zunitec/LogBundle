{
    "name": "zuni/log-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Logging all INSERT, UPDATE and DELETE actions from doctrine events.",
    "keywords": ["log", "logging", "save"],
    "homepage": "https://github.com/zunitec/LogBundle.git",
    "authors": [
        {
            "name": "Zunitec Tecnologias",
            "email": "zunitec@zunitec.com.br",
            "homepage": "http://zunitec.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "zuni/enum-bundle": "1.0.*@dev"
    },
    "require-dev": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "zuni/enum-bundle": "1.0.*@dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "Zuni\\LogBundle": "" }
    },
    "target-dir": "Zuni/LogBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
